{
  "manifest_version": 3,
  "version": "1.0.0",
  "name": "Eng help 1",
  "description": "A chrome extension 1",
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "Ctrl+K": {
      "suggested_key": {
        "default": "Ctrl+K",
        "mac": "Command+K"
      },
      "description": "Ctrl+K."
    },
    "Ctrl+I": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "Command+I"
      },
      "description": "Ctrl+I."
    }
  },
  "permissions": ["contextMenus", "search", "tabs", "webNavigation", "activeTab", "scripting"],
  "action": {
    "default_icon": {
      "16": "img/icon.png",
      "24": "img/icon.png",
      "32": "img/icon.png"
    }
  }
}

